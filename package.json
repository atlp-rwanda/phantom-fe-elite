{
	"name": "setup-react-project",
	"version": "1.0.0",
	"description": "setting up the react project for phantom app",
	"main": "index.js",
	"jest": {
		"transform": {
			"\\.js$": "<rootDir>/node_modules/babel-jest"
		},
		"collectCoverageFrom": [
			"src/**/*.{js, jsx, ts, tsx}",
			"!src/component/starter-files-draft/NumCounter.js",
			"!src/redux/actions/counterActions.js",
			"!src/redux/constants/action-types.js",
			"!src/redux/reducers/counterReducer.js",
			"!src/redux/store/store.js",
			"!src/redux/reducers/rootReducer.js",
			"!src/index.js",
			"!src/postcss.config.js",
			"!src/redux/actions/rootAction/index.js",
			"!src/redux/reducers/RootReducer/index.js",
			"!src/redux/reducers/authLoginReducer.js",
			"!src/redux/store/index.js"
		],
		"transformIgnorePatterns": [
			"node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
		]
	},
	"scripts": {
		"start": "webpack serve --mode development --open",
		"build": "webpack --mode production",
		"test": "react-scripts test --watchAll=false",
		"coverage": "react-scripts test --coverage --watchAll=false",
		"db": "json-server --watch --port 7000 data/db.json"
	},
	"keywords": [
		"webpack5",
		"babel",
		"react"
	],
	"author": "elite team member",
	"license": "ISC",
	"dependencies": {
		"@testing-library/user-event": "^14.1.1",
		"autoprefixer": "^10.2.1",
		"axios": "^0.27.2",
		"babel-jest": "^27.5.1",
